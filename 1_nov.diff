+                    }
+                    
+                    if (obs.y > canvas.height) {
+                        obstacles.splice(i, 1);
+                        gameState.score += 10;
+                    }
+                }
+                
+                drawPlayer();
+                
+                gameState.distance += gameState.speed;
+                updateUI();
+            }
+            
+            requestAnimationFrame(gameLoop);
+        }
+
+        // ============= Start Game =============
+        updateHearts();
+        gameLoop();
+    }
