+                        gameState.speed += 0.03;
+                    }
+                }
+                
+                updateDeathAnimation();
+                
+                for (var i = obstacles.length - 1; i >= 0; i--) {
+                    var obs = obstacles[i];
+                    updateObstacle(obs);
+                    drawObstacle(obs);
+                    
+                    if (checkCollision(obs) && !player.isDying) {
+                        if (obs.isKey && !obs.isFake) {
+                            gameState.hasKey = true;
+                            gameState.score += 500;
+                            gameState.gameOver = true;
+                            finalScoreP.textContent = 'Final Score: ' + gameState.score;
+                            victoryDiv.style.display = 'block';
+                        } else {
+                            handleHit();
+                            obstacles.splice(i, 1);
